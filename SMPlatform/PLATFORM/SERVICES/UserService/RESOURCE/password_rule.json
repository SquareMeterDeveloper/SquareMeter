{
    //格式要求
    "format": {
        //最小长度
        "min_length"               : 8,
        //最大长度
        "max_length"               : 20,
        //是否必须有数字
        "is_must_number"           : true,
        //是否必须有大写字符
        "is_must_uppercase"        : true,
        //是否必须有小写字符
        "is_must_lowercase"        : true,
        //是否必须有特殊字符
        "is_must_special_character": true
    }
}